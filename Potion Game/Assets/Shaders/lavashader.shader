					float3 WorldTangent = -cross(GetObjectToWorldMatrix()._13_23_33, WorldNormal);
					float3 WorldBiTangent = cross(WorldNormal, -WorldTangent);
				#else
					float3 WorldNormal = normalize( IN.tSpace0.xyz );
					float3 WorldTangent = IN.tSpace1.xyz;
					float3 WorldBiTangent = IN.tSpace2.xyz;
				#endif
				float3 WorldPosition = float3(IN.tSpace0.w,IN.tSpace1.w,IN.tSpace2.w);
				float3 WorldViewDirection = _WorldSpaceCameraPos.xyz  - WorldPosition;
				float4 ShadowCoords = float4( 0, 0, 0, 0 );
				#if defined(ASE_NEEDS_FRAG_SCREEN_POSITION)
				float4 ScreenPos = IN.screenPos;
				#endif

				#if defined(REQUIRES_VERTEX_SHADOW_COORD_INTERPOLATOR)
					ShadowCoords = IN.shadowCoord;
				#elif defined(MAIN_LIGHT_CALCULATE_SHADOWS)
					ShadowCoords = TransformWorldToShadowCoord( WorldPosition );
				#endif
	
				WorldViewDirection = SafeNormalize( WorldViewDirection );

				float2 texCoord23 = IN.ase_texcoord7.xy * float2( 1,1 ) + float2( 0,0 );
				float2 uv_flowmap = IN.ase_texcoord7.xy * _flowmap_ST.xy + _flowmap_ST.zw;
				float4 blendOpSrc24 = float4( texCoord23, 0.0 , 0.0 );
				float4 blendOpDest24 = (tex2D( _flowmap, uv_flowmap )).rgba;
				float4 temp_output_24_0 = ( saturate( ( 1.0 - ( ( 1.0 - blendOpDest24) / max( blendOpSrc24, 0.00001) ) ) ));
				float temp_output_19_0 = ( _TimeParameters.x * _Float1 );
				float temp_output_1_0_g2 = temp_output_19_0;
				float temp_output_21_0 = (0.0 + (( ( temp_output_1_0_g2 - floor( ( temp_output_1_0_g2 + 0.5 ) ) ) * 2 ) - -1.0) * (0.64 - 0.0) / (1.0 - -1.0));
				float A29 = -temp_output_21_0;
				float4 lerpResult25 = lerp( float4( texCoord23, 0.0 , 0.0 ) , temp_output_24_0 , A29);
				float2 temp_cast_2 = (_tiling).xx;
				float2 texCoord34 = IN.ase_texcoord7.xy * temp_cast_2 + float2( 0,0 );
				float2 tiling35 = texCoord34;
				float4 flowa47 = ( lerpResult25 + float4( tiling35, 0.0 , 0.0 ) );
				float temp_output_1_0_g3 = (temp_output_19_0*1.0 + 0.5);
				float B42 = -(0.0 + (( ( temp_output_1_0_g3 - floor( ( temp_output_1_0_g3 + 0.5 ) ) ) * 2 ) - -1.0) * (0.64 - 0.0) / (1.0 - -1.0));
				float4 lerpResult43 = lerp( float4( texCoord23, 0.0 , 0.0 ) , temp_output_24_0 , B42);
				float4 flowb48 = ( lerpResult43 + float4( tiling35, 0.0 , 0.0 ) );
				float blendtime55 = abs( ( 1.0 - ( temp_output_21_0 / _div ) ) );
				float4 lerpResult74 = lerp( ( tex2D( _Texture2, flowa47.rg ) * _emmcolor ) , ( tex2D( _Texture2, flowb48.rg ) * _emmcolor ) , blendtime55);
				float4 emm75 = lerpResult74;
				
				float3 Albedo = float3(0.5, 0.5, 0.5);
				float3 Normal = float3(0, 0, 1);
				float3 Emission = emm75.rgb;
				float3 Specular = 0.5;
				float Metallic = 0;
				float Smoothness = 0.5;
				float Occlusion = 1;
				float Alpha = 1;
				float AlphaClipThreshold = 0.5;
				float AlphaClipThresholdShadow = 0.5;
				float3 BakedGI = 0;
				float3 RefractionColor = 1;
				float RefractionIndex = 1;
				float3 Transmission = 1;
				float3 Translucency = 1;
				#ifdef ASE_DEPTH_WRITE_ON
				float DepthValue = 0;
				#endif

				#ifdef _ALPHATEST_ON
					clip(Alpha - AlphaClipThreshold);
				#endif

				InputData inputData;
				inputData.positionWS = WorldPosition;
				inputData.viewDirectionWS = WorldViewDirection;
				inputData.shadowCoord = ShadowCoords;

				#ifdef _NORMALMAP
					#if _NORMAL_DROPOFF_TS
					inputData.normalWS = TransformTangentToWorld(Normal, half3x3( WorldTangent, WorldBiTangent, WorldNormal ));
					#elif _NORMAL_DROPOFF_OS
					inputData.normalWS = TransformObjectToWorldNormal(Normal);
					#elif _NORMAL_DROPOFF_WS
					inputData.normalWS = Normal;
					#endif
					inputData.normalWS = NormalizeNormalPerPixel(inputData.normalWS);
				#else
					inputData.normalWS = WorldNormal;
				#endif

				#ifdef ASE_FOG
					inputData.fogCoord = IN.fogFactorAndVertexLight.x;
				#endif

