			float3 MyCustomExpression248( float3 impactNormal, float3 vertPos, float sphereRadius )
			{
				float height = dot(vertPos, impactNormal);
				   float3 cp = impactNormal * height; 
				   float3 vpcp = (vertPos - cp);
				   float b1 = length(vpcp);
				   float b2 = sqrt((sphereRadius * sphereRadius) - (height * height));
				   float d = b2 - b1; 
				   float3 outPos = vertPos + (d * normalize(vpcp)); 
